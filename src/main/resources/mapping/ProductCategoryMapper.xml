<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.zhangbiao.sell.mapper.ProductCategoryMapper">

    <select id="selectByCategoryType"
            resultType="co.zhangbiao.sell.entity.ProductCategory">
        select * from product_category
        where category_type = #{categoryType}
    </select>

</mapper>